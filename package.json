{
  "name": "ts-react-boilerplate",
  "version": "1.0.0",
  "description": "Another basic Typescript, React boilerplate",
  "main": "src/index.tsx",
  "author": "Brendon <frontalspy@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"npm run start:app\" \"npm run start:server\"",
    "start:app": "vite --config ./config/vite.config.ts",
    "start:server": "nodemon --esm ./src/server/index.ts",
    "test": "jest --config jest.json"
  },
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.8"
  },
  "type": "module",
  "devDependencies": {
    "@swc/core": "^1.3.39",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  }
}
